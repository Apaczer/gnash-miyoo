	db2x_xsltproc -s texi $(srcdir)/gnash.xml -o gnash.txml
	db2x_texixml gnash.txml
	mv gnash_manual_v*.texi gnash.texi
	-@$(RM) gnash.txml

.texi.info:
	makeinfo --force gnash.texi
	mv gnash_manual_v*.info gnash.info
	-@$(RM) gnash.texi

