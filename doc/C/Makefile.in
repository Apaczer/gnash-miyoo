.xml.html:
	@echo "Making HTML from XML source..."
	@xsltproc --output $@ --nonet $(STYLESDIR)/html/docbook.xsl $<

.xml.pdf:
	@echo "Making PDF from XML source..."
	@fop.sh -xsl $(STYLESDIR)/fo/docbook.xsl -xml $< -pdf $@

