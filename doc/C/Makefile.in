#	@xsltproc --output pgd.1 --nonet $(STYLESDIR)/manpages/docbook.xsl $(srcdir)/gnash-man.xml
	db2x_xsltproc -s man $(srcdir)/gnash-man.xml -o gnash.mxml
	db2x_manxml gnash.mxml
	-@$(RM) gnash.mxml

